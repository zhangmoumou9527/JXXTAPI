{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  //配置数据库
  "dbConfigs": [
    {
      "Conn": "Data Source=127.0.0.1;User ID=sa;Password=123;Initial Catalog=JXXT;",
      "DbType": 1, //数据库类型 MySql = 0, SqlServer = 1, Oracle = 3，PgSql = 4
      "ConfigId": "0", //多租户唯一标识
      "IsAutoCloseConnection": true
    }
  ],
  //代码生成数据库配置
  "CodeGenDbConfig": {
    //代码生成连接字符串，注意{dbName}为固定格式，不要填写数据库名
    "Conn": "Data Source=127.0.0.1;User ID=sa;Password=123;Initial Catalog={dbName};",
    "DbType": 1,
    "IsAutoCloseConnection": true,
    "DbName": "JXXT" //代码生成默认连接数据库
  },





  "AllowedHosts": "*"
}
